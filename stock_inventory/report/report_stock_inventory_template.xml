        <report string="Imprimir reporte de inventario" id="action_agr_invoice_report" model="stock.inventory" report_type="qweb-pdf" name="stock_inventory.report_stock_inventory" file="stock_inventory.report_stock_inventory" print_report_name="'Reporte de inventario - %s - %s' % (object.owner_id.name or '', object.name)" paperformat="paperformat_report_stock_inventory" menu="True" />
